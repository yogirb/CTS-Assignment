<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<welcome-file-list>
		<welcome-file>login.html</welcome-file>

	</welcome-file-list>

	<context-param>
		<param-name>userName</param-name>
		<param-value>root</param-value>
	</context-param>

	<context-param>
		<param-name>password</param-name>
		<param-value>root</param-value>
	</context-param>

	<context-param>
		<param-name>url</param-name>
		<param-value>jdbc:mysql://localhost/ctspune</param-value>
	</context-param>
	<context-param>

		<param-name>driver</param-name>
		<param-value>com.mysql.jdbc.Driver</param-value>
	</context-param>


	<servlet>
		<servlet-name>LoginExample2</servlet-name>
		<servlet-class>com.cognizant.work.LoginExample2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginExample2</servlet-name>
		<url-pattern>/cs2</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginExample</servlet-name>
		<servlet-class>com.cognizant.work.LoginExample</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>LoginExample</servlet-name>
		<url-pattern>/cse</url-pattern>
	</servlet-mapping>

</web-app>